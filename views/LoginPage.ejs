  <link rel="stylesheet" type="text/css" href="Tiss.css"/> 
  <link rel="icon" type="image/x-icon" href="Photo/dudtled.png">

  <title>LoginPage</title>

</head>
<body class="page" data-spy="scroll" data-target=".navbar" onload="startTime()">

    <%- include('partials/navbar') %>

                                              <!-- MODAL -->
	<div class="modal fade" id="modall">
		<div class="modal-dialog modal-sm modal-dialog-centered">
			<div class="modal-content">
				<div class="modal-header">
					<h4 class="modal-title">Radiation Risk <i class="fas fa-biohazard"></i></h4>
					<button type="button" class="close" data-dismiss="modal">&times;</button>
				</div>
				<div class="modal-body font-weight-bold">
					Radiation spread to this tab as a result of a minor accident. <br> We are working on it, we will clean it very soon
				</div>
				<div class="modal-footer">
					<button class="btn btn-danger" type="button" data-dismiss="modal">Run <i class="fas fa-running"></i></button>
				</div>
			</div>
		</div>
	</div>
                  <!-- MODAL END  -->

    <div class="container mt-5 pt-5 sign">
      <div class="wrap d-md-flex">
        <img class="w-50" src="Photo/war.jpg" alt="">
        <div class="p-4 p-md-5">
          <div class="d-flex">
            <div class="w-100">
              <h3 class="mb-4" style="text-align: end;">Sign In</h3>
            </div>
          </div>
          <form method="POST" action="/login" class="signin-form">
            <div class="form-group mb-3">
              <label class="label">Username</label>
              <input name="name" type="text" class="form-control" placeholder="Username" required>
            </div>
            <div class="form-group mb-3">
              <label class="label">Password</label>
              <input name="password" type="password" class="form-control" placeholder="Password" required>
            </div>
            <div class="form-group">
              <button type="submit" class="form-control btn btn-warning rounded submit px-3">Sign In</button>
            </div>
            <div class="form-group d-md-flex">
              <div class="w-75 text-left">
                <label class="checkbox-wrap checkbox-primary">Remember Me
                <input type="checkbox" checked>
                <span class="checkmark"></span>
                </label>
              </div>
              <div class="w-25 text-md-right">
                <a href="reset">Forgot Password</a>
              </div>
            </div>
          </form>
          <p class="text-center">Not a member? <a href="Signup">Sign Up</a></p>
        </div>
      </div>
    </div>

                          <!-- Reviews End  -->
                          
<footer id="more" class="py-4 mt-5 bg-bt text-white text-center" class="paged">

